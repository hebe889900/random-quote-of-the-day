    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "react-social-icons": "^2.8.1",
    "wepback": "^1.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "decode-html": "^2.0.0",
    "he": "^1.1.1",
